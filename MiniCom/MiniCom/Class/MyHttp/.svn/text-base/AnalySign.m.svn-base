#import "AnalySign.h"
#import "Singleton.h"
#import "Config.h"
#import "SignView.h"

@implementation AnalySign
- (void)analyData:(NSDictionary *)dic
{
    Singleton *sl = [Singleton SharedInstance];
    int response = [[dic objectForKey:@"response"] intValue];
    if (response == 1) {
        NSString *act = [dic objectForKey:@"act"];
        SignView *signView = [sl.sysViewDic valueForKey:[NSString stringWithFormat:@"%d",SP_Sign]];
        if ([act isEqualToString:@"everyday"]) {
            [signView refreshEverySignView:dic];
        }
        else if ([act isEqualToString:@"luck"]) {
            [signView refreshluckySignView:dic];
        }
    }
    else
    {
        NSLog(@"AnalySign error");
    }
}
@end
